<?xml version="1.0" encoding="utf-8" ?>
<OracleConfiguration xmlns="http://husqvik.com/SqlPad/2014/08/Oracle">
  <StartupScript>
    ALTER SESSION SET optimizer_dynamic_sampling = 0 plsql_debug = true;
    
    BEGIN
      utl_http.set_body_charset('UTF8');
      utl_http.set_wallet(path => 'file:C:/oracle/wallet');
    END;
  </StartupScript>
  <TKProfPath>C:\Oracle\product\12.1.0\dbhome_1\BIN\tkprof.exe</TKProfPath>
  <Connections>
    <Connection ConnectionName="*" RemoteTraceDirectory="c:\Oracle\diag\rdbms\hq12c\hq12c\trace\">
      <StartupScript></StartupScript>
      <ExecutionPlan>
        <TargetTable Name="TOAD_PLAN_TABLE" />
      </ExecutionPlan>
    </Connection>
  </Connections>
  <Formatter>
    <FormatOptions Identifier="Lower" Alias="Lower" Keyword="Upper" ReservedWord="Upper" />
  </Formatter>
</OracleConfiguration>
